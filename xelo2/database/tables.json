    "channels": {
        "channel_group_id": null,
        "name": {
            "name": "name",
            "doc": "Label of the channel. The label must correspond to _electrodes.tsv name and all ieeg type channels are required to have a position.",
            "type": "TEXT"
        },
        "type": {
            "name": "type",
            "doc": "Type of channel.",
            "type": "TEXT",
            "values": [
                "EEG",
                "ECOG",
                "SEEG",
                "EOG",
                "ECG",
                "EMG",
                "TRIG",
                "MISC",
                "REF",
                "OTHER"
            ]
        },
        "units": {
            "name": "units",
            "doc": "Physical unit of the value represented in this channel, e.g., V for Volt, specified according to the SI unit symbol and possibly prefix symbol (e.g., mV, μV). Note that 'micro' can be encoded as 'CEBC' or 'C2B5' in utf-8.",
            "type": "TEXT",
            "values": [
                "V",
                "mV",
                "μV",
                "µV",
                "bpm",
                "%"
            ]
        },
        "low_cutoff": {
            "name": "Low CutOff",
            "doc": "Frequencies used for the low pass filter applied to the channel in Hz. If no low pass filter was applied, use n/a. Note that anti-alias is a low pass filter, specify its frequencies here if applicable.",
            "type": "FLOAT"
        },
        "high_cutoff": {
            "name": "High CutOff",
            "doc": "Frequencies used for the high pass filter applied to the channel in Hz. If no high pass filter applied, use n/a.",
            "type": "FLOAT"
        },
        "reference": {
            "name": "Reference",
            "doc": "Specification of the reference (e.g., 'mastoid', 'ElectrodeName01', 'intracranial', 'CAR', 'other', 'n/a'). If the channel is not an electrode channel (e.g., a microphone channel) use n/a",
            "type": "TEXT"
        },
        "groups": {
            "name": "Group",
            "doc": "Which group of channels (grid/strip/seeg/depth) this channel belongs to. This is relevant because one group has one cable-bundle and noise can be shared. This can be a name or number. ",
            "type": "TEXT"
        },
        "sampling_frequency": {
            "name": "Sampling Frequency",
            "doc": "Sampling rate of the channel in Hz.",
            "type": "FLOAT"
        },
        "description": {
            "name": "Description",
            "doc": "Brief free-text description of the channel, or other information of interest (e.g., position (e.g., 'left lateral temporal surface', etc.).",
            "type": "TEXT"
        },
        "notch": {
            "name": "Notch",
            "doc": "Frequencies used for the notch filter applied to the channel, in Hz. If no notch filter applied, use n/a.",
            "type": "FLOAT"
        },
        "status": {
            "name": "Status",
            "doc": "Data quality observed on the channel (good/bad). A channel is considered bad if its data quality is compromised by excessive noise. Description of noise type SHOULD be provided in [status_description].",
            "type": "TEXT"
        },
        "status_description": {
            "name": "Status Description",
            "doc": "Freeform text description of noise or artifact affecting data quality on the channel. It is meant to explain why the channel was declared bad in [status].",
            "type": "TEXT"
        }
    },
