        },
        "impedance": {
            "name": "Impedance",
            "doc": "Impedance of the electrode in kOhm.",
            "type": "FLOAT"
        },
        "dimension": {
            "name": "Dimension",
            "doc": "Size of the group (grid/strip/probe) that this electrode belongs to. Must be of form [AxB] with the smallest dimension first (e.g., [1x8]).",
            "type": "TEXT"
        }
