    "channel_groups": {
        "id": null,
        "Reference": {
            "name": "Reference",
            "type": "TEXT",
            "doc": "General description of the reference scheme used and (when applicable) of location of the reference electrode in the raw recordings (e.g., 'left mastoid', 'bipolar', 'T01' for electrode with name T01, 'intracranial electrode on top of a grid, not included with data', 'upside down electrode')."
        }
